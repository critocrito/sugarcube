  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint && npm run test",
    "check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "docs": "node ./scripts/make-docs.js",
    "lint": "eslint packages/**/lib/**/*.js test/**/*.js boilerplate-plugin/lib/**/*.js",
    "fix": "npm run lint -- --fix",
    "clean": "rimraf packages/**/_dist",
    "clean-mods": "lerna clean --yes",
    "compile": "cross-env NODE_ENV=production make compile",
    "coverage": "cross-env NODE_ENV=test nyc npm test",
    "prerelease": "npm run test && npm run test:e2e",
    "release": "npm run build && lerna publish --conventional-commits -m \"chore(release): Bumped to version %s.\"",
    "setup": "npm run clean-mods && make setup && npm run compile",
    "test": "npm run test:mocha",
    "test:e2e": "cd project && npm run test-parallel",
    "test:mocha": "cross-env NODE_ENV=test mocha -c -R progress -t 6000 -r @babel/register -r test/helpers.js test/**/*.spec.js",
    "watch": "make watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "dashp": "^0.10.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "husky": "^2.4.1",
    "jsverify": "^0.8.4",
    "lerna": "^3.15.0",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.11",
    "mdast-util-to-string": "^1.0.5",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "nyc": "^12.0.2",
    "prettier": "^1.14.3",
    "remark-parse": "^6.0.3",
    "remark-stringify": "^6.0.4",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "unified": "^7.1.0",
    "unist-builder": "^1.0.3",
    "unist-util-find": "^1.0.1",
    "unist-util-find-after": "^2.0.2",
    "unist-util-find-all-after": "^1.0.3",
    "unist-util-find-all-between": "^1.0.2",
    "validate-commit-msg": "^2.14.0"
  }
}
